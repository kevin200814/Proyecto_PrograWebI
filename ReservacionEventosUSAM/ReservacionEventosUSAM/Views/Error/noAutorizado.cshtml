
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>noAutorizado</title>
    <link href="~/Content/Style.css" rel="stylesheet" />
    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css">
    <link href="~/Content/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
</head>
<body>
    <div class="container">
        <section class="content-header">
            <h2>noAutorizado</h2>
        </section>

        <section class="content">
            <div class="error-page">
                <div class="error-content">
                    <h2 class="headline text-red">Error</h2>
                    <h3>
                        <i class="fa fa-warning text-red">Sin Autorizacion</i>
                    </h3>
                    @if (ViewBag.Operacion != "")
            { <p>Operacion @ViewBag.Operacion</p> }
                    @if (ViewBag.Modulo != "")
            { <p>Operacion @ViewBag.Modulo</p>    }
                    @if (ViewBag.msjerror != "")
            { <p>Operacion @ViewBag.msjerror</p>  }
                </div>
            </div>
        </section>

        <div class="message" style="background:url(../../images/señal.png) 50% no-repeat;">

            <h1>404</h1>
            <h2>There's nothing here.</h2>
            <h3>Find what you're looking for with our app.</h3>

        </div>
    </div>
    
</body>
</html>
